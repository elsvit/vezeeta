    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.3.0",
    "classnames": "^2.2.5",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
